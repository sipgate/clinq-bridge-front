{
  "name": "clinq-bridge-front",
  "version": "1.0.0",
  "private": true,
  "description": "CLINQ Bridge for Front",
  "main": "dist/index.js",
  "repository": "git@github.com:sipgate/clinq-bridge-front.git",
  "contributors": [
    {
      "name": "Oliver Wegner",
      "email": "oliver@lambda-startup.com"
    }
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index",
    "watch": "ts-node-dev --respawn src/index.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "@clinq/bridge": "^11.2.4",
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.7",
    "axios": "^0.19.0",
    "bunyan": "^1.8.12",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "husky": "^2.4.0",
    "lint-staged": "^8.2.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.2.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "tslint",
      "prettier --write",
      "git add"
    ],
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
